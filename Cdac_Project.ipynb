    "# Function to count NER\n",
    "def ner(tweet_full_text):\n",
    "\n",
    "    # Create spacy object\n",
    "    doc = nlp(tweet_full_text)\n",
    "\n",
    "    # Variable to store the count of POS tags\n",
    "    count = 0\n",
    "\n",
    "    # Iterate over the tokens\n",
    "    for ent in doc.ents:\n",
    "        # Increment counter if token is a NER\n",
    "        if ent.label_:\n",
    "            count += 1\n",
    "    # Return count\n",
    "    return count"
   ]
