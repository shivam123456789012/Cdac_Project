   "source": [
    "# Function to count the number of stopwords in Tweets\n",
    "def stopwords(tweet_full_text):\n",
    "\n",
    "    # Create spacy object\n",
    "    doc = nlp(tweet_full_text)\n",
    "\n",
    "    # Variable to store count of stopwords\n",
    "    count = 0\n",
    "    for token in doc:\n",
    "        if token.is_stop == True:\n",
    "            count += 1\n",
    "    return count"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "3acf7fdf",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Apply function\n",
    "df['stopwords'] = df['tweet_full_text'].apply(stopwords)"
   ]
  },
