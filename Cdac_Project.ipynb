   "source": [
    "# Preprocess tweets\n",
    "def preprocess(tweet_full_text):\n",
    "\n",
    "    # Remove unicode characters\n",
    "    tweet_full_text = re.sub(r\"<U\\+[A-Z0-9]+>|<ed>\", \"\", tweet_full_text)\n",
    "    # Remove newline and rawstring characters\n",
    "    tweet_full_text = re.sub(r\"\\n|\\r\", \"\", tweet_full_text)\n",
    "\n",
    "    return tweet_full_text"
   ]
  },
